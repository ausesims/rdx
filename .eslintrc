{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "globals": {
    "process": {}
  },
  "env": {
    "es6": true
  },
  "ecmaFeatures": {
    "classes": true,
    "modules": true,
    "jsx": true
  },
  "rules": {
    "array-callback-return": 0,
    "arrow-body-style": 0,
    "arrow-parens": ["error", "as-needed"],
    "arrow-spacing": 1,
    "brace-style": [
      1,
      "1tbs"
    ],
    "class-methods-use-this": 0,
    "curly": [
      1,
      "multi-line"
    ],
    "comma-dangle": [
      1,
      "never"
    ],
    "comma-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "consistent-return": 0,
    "func-names": 0,
    "global-require": 0,
    "guard-for-in": 0,
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "jsx-quotes": [
      1,
      "prefer-single"
    ],
    "key-spacing": [
      0,
      {
        "beforeColon": true,
        "afterColon": true
      }
    ],
    "keyword-spacing": 1,
    "max-len": [
      1,
      120
    ],
    "new-cap": 0,
    "no-alert": 1,
    "no-confusing-arrow": 0,
    "no-else-return": 0,
    "no-extra-parens": 0,
    "no-multiple-empty-lines": 1,
    "no-multi-spaces": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": [
      1,
      {
        "props": false
      }
    ],
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "no-restricted-syntax": [0],
    "no-return-assign": 0,
    "no-return-await": 0,
    "no-shadow": 0,
    "no-spaced-func": 1,
    "no-trailing-spaces": 0,
    "no-undef": 2,
    "no-unneeded-ternary": 0,
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "no-use-before-define": 2,
    "no-useless-constructor": 0,
    "no-var": 0,
    "no-warning-comments": [
      1,
      {
        "terms": [
          "todo",
          "fixme",
          "xxx"
        ],
        "location": "start"
      }
    ],
    "object-curly-spacing": 0,
    "object-shorthand": 0,
    "one-var": [
      0,
      {
        "var": "always",
        "let": "always",
        "const": "always"
      }
    ],
    "one-var-declaration-per-line": 0,
    "padded-blocks": 0,
    "prefer-arrow-callback": 1,
    "prefer-const": 1,
    "prefer-template": 1,
    "quotes": [
      1,
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "semi": 1,
    "space-before-blocks": [
      1,
      "always"
    ],
    "space-before-function-paren": [
      0,
      "always"
    ],
    "space-in-parens": [
      1,
      "never"
    ],
    "spaced-comment": 1,
    "radix": 1,
    "yoda": 0,


    "import/no-dynamic-require": 0,


    "react/jsx-boolean-value": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-indent-props": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-quotes": [
      0,
      "single"
    ],
    "react/jsx-space-before-closing": 0,
    "react/prop-types": 1,
    "react/sort-comp": 0
  }
}
